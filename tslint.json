{
  "extends": ["tslint:latest", "tslint-config-prettier"],

  "rulesDirectory": ["tslint-plugin-prettier"],

  "rules": {
    "prettier": true,
    "interface-name": [true, "never-prefix"],
    "no-empty-interface": false,
    "no-shadowed-variable": false,
    "variable-name": false,
    "no-var-requires": false
  }
}
